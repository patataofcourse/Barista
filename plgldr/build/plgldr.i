static inline u32 CSND_VOL(float vol, float pan)
{
 float rpan;
 u32 lvol, rvol;

 if (vol < 0.0f) vol = 0.0f;
 else if (vol > 1.0f) vol = 1.0f;

 rpan = (pan+1) / 2;
 if (rpan < 0.0f) rpan = 0.0f;
 else if (rpan > 1.0f) rpan = 1.0f;

 lvol = vol*(1-rpan) * 0x8000;
 rvol = vol*rpan * 0x8000;
 return lvol | (rvol << 16);
}


